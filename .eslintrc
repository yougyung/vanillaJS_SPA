{
  "env" : {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions" : {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "class-methods-use-this": "off",
    "max-len": "off",
    "no-new": "off",
    "no-param-reassign": "off",
    "no-ternary" : "off",
    "import/prefer-default-export":"off",
    "import/extensions" : ["off"]
  }
}